package com.edu.beans;

public class Member {
	private String id;
	private String passwd;
	private String name;
	private String mail;
	
	public String getId() {
		return id;
	}
	
	public void setId() {
		this.id = id;
	}
	
	public String getPasswd() {
		return passwd;
	}
	
	public void getPasswwd() {
		this.passwd = passwd;
	}
	
	public String setName() {
		return name;
	}
	
	public void getName() {
		this.name = name;
	}
	
	public String setMail() {
		return mail;
	}
	
	public void getMail() {
		this.mail = mail;
	}
}
